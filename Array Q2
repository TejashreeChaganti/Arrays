#include <iostream>
using namespace std;

int main() {
    int numbers[10];
    int sum = 0;

    cout << "Enter 10 numbers:" << endl;
    for (int i = 0; i < 10; i++) {
        cout << "Number " << i + 1 << ": ";
        cin >> numbers[i];
        sum += numbers[i];
    }

    cout << "Sum of the entered numbers is: " << sum << endl;

    return 0;
}
