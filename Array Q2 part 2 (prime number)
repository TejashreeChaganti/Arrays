#include <iostream>
#include <cmath>
using namespace std;

bool isPrime(int num) {
    if (num <= 1) return false;
    for (int i = 2; i <= sqrt(num); i++) {
        if (num % i == 0)
            return false;
    }
    return true;
}

int main() {
    int arr[10];
    int totalSum = 0;
    int primeCount = 0;

    cout << "Enter 10 numbers:" << endl;
    for (int i = 0; i < 10; i++) {
        cout << "Number " << i + 1 << ": ";
        cin >> arr[i];
        totalSum += arr[i];

        if (isPrime(arr[i]))
            primeCount++;
    }

    cout << "\nSum of all numbers: " << totalSum << endl;
    cout << "Count of prime numbers: " << primeCount << endl;

    return 0;
}
