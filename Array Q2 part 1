#include <iostream>
using namespace std;

int main() {
    int arr[10];
    int sum = 0;
    int twoDigitEvenCount = 0;

    cout << "Enter 10 numbers:" << endl;
    for (int i = 0; i < 10; i++) {
        cout << "Number " << i + 1 << ": ";
        cin >> arr[i];
        sum += arr[i];
        
        if ((arr[i] >= 10 && arr[i] <= 99 || arr[i] <= -10 && arr[i] >= -99) && arr[i] % 2 == 0) {
            twoDigitEvenCount++;
        }
    }

    cout << "\nSum of all numbers: " << sum << endl;
    cout << "Count of two-digit even numbers: " << twoDigitEvenCount << endl;

    return 0;
}
